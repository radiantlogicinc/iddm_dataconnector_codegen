{
  "name": "Harry Potter OpenAPI Connector",
  "description": "Unified connector for Harry Potter books and characters",
  "backendCategory": "custom",
  "userCreated": true,
  "icon": "icons/harry_potter.png",
  "isSchemaExtractable": false,
  "meta": [
    {
      "name": "hostname",
      "description": "Hostname of the Harry Potter OpenAPI service",
      "sectionName": "Connection",
      "defaultValue": "localhost",
      "dataType": "STRING",
      "isRequired": true,
      "regex": "^[a-zA-Z0-9.-]+$"
    },
    {
      "name": "port",
      "description": "Port of the OpenAPI service",
      "sectionName": "Connection",
      "defaultValue": "8080",
      "dataType": "STRING",
      "isRequired": true,
      "regex": "^[0-9]{1,5}$"
    },
    {
      "name": "apiKey",
      "description": "API key for authenticating to the OpenAPI",
      "sectionName": "Authentication",
      "defaultValue": "",
      "dataType": "STRING",
      "isRequired": false,
      "regex": ".*"
    }
  ]
}